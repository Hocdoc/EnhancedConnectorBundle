{
    "name": "akeneo-labs/pim-enhanced-connector",
    "type": "symfony-bundle",
    "description": "Provides PIM product reader with more option (choice on completeness, choice on enabled, delta based on last export time)",
    "keywords": ["pim", "product", "export", "delta"],
    "homepage": "http://www.akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name":     "Benoit Jacquemont",
            "email":    "benoit@akeneo.com",
            "homepage": "http://www.akeneo.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/platform.git"
        }
    ],
    "require": {
        "akeneo/pim-community-dev": "1.4.*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev",
        "henrikbjorn/phpspec-code-coverage": "1.0.*@dev",
        "fabpot/php-cs-fixer": "@stable",
        "squizlabs/php_codesniffer": "1.*",
        "pdepend/pdepend": "1.1.*",
        "phpmd/phpmd": "1.*"
    },
    "autoload": {
        "psr-0": { "Pim\\Bundle\\EnhancedConnectorBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "target-dir": "Pim/Bundle/EnhancedConnectorBundle"
}
